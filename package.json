{
  "name": "express-es6",
  "version": "1.0.0",
  "description": "express.js server starter with ES6",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node app.js",
    "dev": "export NODE_ENV=development && nodemon --exec babel-node app.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "author": "Lambert Lin",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
